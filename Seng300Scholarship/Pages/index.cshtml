@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="login-box">
    <form id="form" class="form">
        <h2>Login</h2>
        <div class="form-c">
            <input type="text" id="username" placeholder="Enter username" />
        </div>
        <div class="form-c">
            <input type="password" id="password" placeholder="Enter password" />
        </div>
        <button type="submit">Submit</button>
    </form>
</div>

